{
  "name": "music",
  "version": "0.0.60",
  "description": "Music theory web app.",
  "main": "index.html",
  "watch": {
    "build": "js/*.js"
  },
  "scripts": {
    "test": "",
    "build:compile": "rollup -c",
    "build:minify": "uglifyjs build/compiled.js --in-source-map build/compiled.js.map --source-map build/compiled.min.js.map -o build/compiled.min.js",
    "build": "npm run build:compile && npm run build:minify",
    "postbuild": "npm version patch --git-tag-version=false",
    "watch": "watch 'npm run build:compile' js/",
    "dev": "npm start & npm run watch",
    "start": "python -m SimpleHTTPServer",
    "clean": "rm build/*"
  },
  "author": "Cory McCartan",
  "repository": "https://github.com/CoryMcCartan/music",
  "license": "ISC",
  "devDependencies": {
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "material-design-lite": "^1.3.0",
    "teoria": "^2.2.0",
    "zepto": "^1.2.0"
  }
}
